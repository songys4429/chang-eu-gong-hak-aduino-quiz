void setup() {
  Serial.begin(9600);
}
void loop() {
  if(Serial.available()>0){
    String a = Serial.readString();
    if(a.indexOf('5')==0) tone(4,523,500); 
    delay(500);
  } 
}

----------------

import processing.serial.*;
Serial p;
void setup() {
  p = new Serial(this,"COM5",9600);
}
void draw() {
}
void keyPressed(){
  background(0);
  textSize(64);
  text(key,64,64);
  p.write(key);  
}
